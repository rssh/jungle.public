<?xml version="1.0"?>
<!-- build file for phpjao.t2 -->
<project name="phpjao.t2" default="php1" basedir="."
 >
    <target name="php1" >
      <exec executable="php" output="phpoutput1.log" >
        <arg value="-c" /> 
        <arg value="php.ini" /> 
        <arg value="php/pa2.php" /> 
      </exec>
    </target>

    <target name="php2" >
      <exec executable="php" output="phpoutput2.log" >
        <arg value="-c" /> 
        <arg value="php.ini" /> 
        <arg value="php/encoding_datetime.php" /> 
      </exec>
    </target>

</project>
