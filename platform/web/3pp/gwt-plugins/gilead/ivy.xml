    <dependency org="net.sf.beanlib" name="beanlib.repackaged" 
       rev="[5.0.1beta,)" conf="core->*" >
       <artifact name="beanlib-hibernate" type="jar" />
    </dependency>
 </dependencies>
</ivy-module>
