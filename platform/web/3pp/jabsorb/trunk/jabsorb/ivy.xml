<ivy-module version="1.4">
    <info organisation="org.jabsorb" module="jabsorb"/>
       
    <configurations>
      <conf name="jar" description="get jar" />
      <conf name="std" description="standard build" />
      <conf name="test" description="test version with junit test" 
                        extends="std" />
      <conf name="doc" description="with tool for building documentations" 
                        extends="std" />
    </configurations>
    <publications>
      <artifact name="jabsorb" type="jar" />
      <artifact name="jsonrpc" type="js" />
      <artifact name="jsonrpc-min" type="js" />
    </publications>
    <dependencies>
        <dependency org="org.jabsorb" name="src" rev="1.3.1"
                    transitive="false" >
         <artifact name="jabsorb-src" type="zip"
           url="http://jabsorb.org/dist/1.3.1/jabsorb-1.3.1-src.zip"
         />
        </dependency>
    </dependencies>
</ivy-module>



