<?xml version="1.0" encoding="utf-8" ?>
<project name="com.googlecode.gwt-log.republish" basedir="."
  xmlns:ivy="antlib:fr.jayasoft.ivy.ant"
 >

  <property file="build.properties" />

  <import file="build-ivy.xml" />

   <target name="publish" depends = "resolve-ivy" >
     <ivy:install from="here.ivy.googlecode.others" 
                  to="datacenter.gradsoft.ua"
                  organisation="com.allen_sauer" module="gwt-log"
                  revision="${origin.version}"
                  />
    </target>

   <target name="publish.local" depends="resolve-ivy" >
     <ivy:install from="here.ivy.googlecode.others" to="jungle.repo"
                  organisation="com.allen_sauer" module="gwt-log"
                  revision="${origin.version}"
                  >
     </ivy:install>
    </target>


</project>
