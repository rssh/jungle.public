<ivy-module version="1.4">
 <info organisation="com.googlecode" module="gwt-math" />
 <configurations>
  <conf name="default" description="default" />
  <conf name="origin" description="origin" />
 </configurations>
 <publications>
   <artifact name="gwt-math" type="jar" />
   <artifact name="gwt-math-server" type="jar" />
 </publications>
 <dependencies>
    <dependency org="com.googlecode" 
               name="gwt-math.origin"
               rev="${origin.version}" 
               conf="origin->*"
               >
       <artifact name="gwt-math" type="jar" 
          url="http://gwt-math.googlecode.com/files/gwt-math-${origin.version}.jar" />
       <artifact name="gwt-math-server" type="jar" 
          url="http://gwt-math.googlecode.com/files/gwt-math-server-${origin.version}.jar" 
       />
    </dependency>
 </dependencies>
</ivy-module>
