<ivy-module version="1.4">
 <info organisation="ua.gradsoft" module="gwt-maps.repackaged" />
  <configurations>
    <conf name="default" description="default repackaged artifacts" />
    <conf name="origin" description="origin artifacts" />
  </configurations>
  <publications>
    <artifact name="gwt-maps" type="jar" />
  </publications>
 <dependencies>
   <dependency org="com.googlecode" 
               name="gwt-google-apis"
               rev="${origin.version}" 
               conf="origin->*"
               >
    <artifact name="gwt-maps" type="zip" 
      url="http://gwt-google-apis.googlecode.com/files/gwt-maps-${origin.version}-rc1.zip" />
   </dependency>
 </dependencies>
</ivy-module>
